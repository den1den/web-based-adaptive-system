<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<parent>
		<groupId>nl.tue.gale</groupId>
		<artifactId>master</artifactId>
		<version>1.2.3</version>
		<relativePath>../master/pom.xml</relativePath>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<artifactId>gale</artifactId>
	<packaging>war</packaging>
	<name>Gale WAR deployment</name>
	<build>
		<finalName>gale</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-deploy-plugin</artifactId>
				<version>2.7</version>
				<configuration>
					<skip>true</skip>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>3.8.1</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>commons-dbcp</groupId>
			<artifactId>commons-dbcp</artifactId>
			<version>1.2.2</version>
			<scope>runtime</scope>
		</dependency>
		<dependency>
			<groupId>hsqldb</groupId>
			<artifactId>hsqldb</artifactId>
			<version>1.8.0.7</version>
			<scope>runtime</scope>
		</dependency>
		<dependency>
			<groupId>c3p0</groupId>
			<artifactId>c3p0</artifactId>
			<version>0.9.1.2</version>
			<scope>runtime</scope>
		</dependency>
		<dependency>
			<groupId>nl.tue.gale</groupId>
			<artifactId>gale-common</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>nl.tue.gale</groupId>
			<artifactId>gale-event</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>nl.tue.gale</groupId>
			<artifactId>gale-dm</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>nl.tue.gale</groupId>
			<artifactId>gale-um</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>nl.tue.gale</groupId>
			<artifactId>gale-ae</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>nl.tue.gale</groupId>
			<artifactId>gale-conversion</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>nl.tue.gale</groupId>
			<artifactId>gale-tools</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>javassist</groupId>
			<artifactId>javassist</artifactId>
			<version>3.10.0.GA</version>
		</dependency>
		<dependency>
			<groupId>nl.tue.gale</groupId>
			<artifactId>gale-geb</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>cglib</groupId>
			<artifactId>cglib</artifactId>
			<version>2.2.2</version>
			<type>jar</type>
			<scope>compile</scope>
		</dependency>
	</dependencies>
</project>